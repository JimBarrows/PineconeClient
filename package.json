{
  "name": "pinecone-client",
  "version": "1.0.0",
  "description": "A React front end for Pinecone.",
  "main": "webpack.config.js",
  "bugs": {
    "url": "https://github.com/JimBarrows/Pinecone-Client/issues"
  },
  "scripts": {
    "docker:push": "docker push registry.gitlab.com/reallybigtree/pinecone-client",
    "docker:build": "docker build -t registry.gitlab.com/reallybigtree/pinecone-client .",
    "docker:run": "docker run --detach --name pinecone-client --label=traefik.backend=client --label=traefik.port=8080 --link pinecone-server:server registry.gitlab.com/reallybigtree/pinecone-client",
    "package": "webpack",
    "start": "webpack-dev-server --hot --inline",
    "test": "wdio wdio.conf.js",
    "test:webdriver:start": "webdriver-manager start",
    "test:update-webdriver": "webdriver-manager update",
    "predocker:build": "npm run pinecone:client:package"
  },
  "author": {
    "name": "Jim Barrows",
    "email": "Jim.Barrows@gmail.com"
  },
  "contributors": [],
  "license": "Apache 2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:JimBarrows/Pinecone-Client.git"
  },
