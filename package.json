    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.16.3",
    "bluebird": "^3.4.6",
    "bootstrap": "^3.3.7",
    "bootstrap-react-components": "^0.15.0",
    "css-loader": "^0.23.1",
    "draft-js": "^0.8.0",
    "fb": "^1.1.1",
    "file-loader": "^0.9.0",
